{
  "name": "@hypercord/bot",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts --format esm --minify",
    "start": "node dist/index.js",
    "register": "tsx scripts/register-commands.ts",
    "watch:register": "chokidar \"src/commands/**/*.{ts,js}\" -c \"pnpm run register\"",
    "dev:hot": "concurrently -n bot,reg -c auto \"pnpm run dev\" \"pnpm run watch:register\"",
    "db:dev": "docker compose -f docker-compose.yaml up -d && prisma migrate dev --name init && prisma generate",
    "lint": "eslint .",
    "test": "vitest run",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "@prisma/client": "^6.16.2",
    "bullmq": "^5.8.1",
    "discord-api-types": "^0.38.26",
    "discord.js": "^14.15.3",
    "dotenv": "^17.2.2",
    "ioredis": "^5.4.1",
    "pino": "^9.5.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.3.3",
    "prisma": "^6.16.2",
    "tsup": "^8.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  }
}
